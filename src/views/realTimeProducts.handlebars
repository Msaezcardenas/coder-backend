<div id='container-products'>

  <form id='form' class='form-product'>
    <div class='container-form'>
      <h2> Crear Producto</h2>
      <div class='form-inputs'>
        <label for='title'>Titulo:</label>
        <input
          type='text'
          id='title'
          name='title'
          required
          placeholder='Titulo'
        /><br />

        <label for='description'>Descripción:</label>
        <input type='text' id='description' name='description' /><br />

        <label for='code'>Código:</label>
        <input type='text' id='code' name='code' /><br />

        <label for='price'>Precio:</label>
        <input type='number' id='price' name='price' /><br />

        <label for='status'>Status:</label>
        <input type='bolean' id='status' name='status' /><br />

        <label for='stock'>Stock:</label>
        <input type='number' id='stock' name='stock' /><br />

        <label for='category'>Categoría:</label>
        <input type='category' id='category' name='category' /><br />

      </div>

      <input type='submit' value='Crear producto' class='btn-submit' />
    </div>
  </form>

  <div id='otro'>

  </div>

</div>

<link rel='stylesheet' href='styles.css' />
<script src='/socket.io/socket.io.js'></script>
<script src='realTimeProducts.js'> </script>